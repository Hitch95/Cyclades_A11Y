{
  "version": 2,
  "name": "cyclades",
  "builds": [
    {
      "src": "cyclades/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "functions": {
    "cyclades-server/deploy.ts": { "runtime": "vercel-deno@3.1.0" }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/cyclades-server/deploy.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}