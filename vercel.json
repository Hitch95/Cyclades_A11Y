{
  "version": 2,
  "name": "cyclades",
  "builds": [
    {
      "src": "cyclades/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "cyclades-server/deploy.ts",
      "use": "@vercel/deno"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/cyclades-server/deploy.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}